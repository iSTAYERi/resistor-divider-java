<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="CENTER" minHeight="300.0" minWidth="500.0" prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <columnConstraints>
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="-Infinity" />
    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="330.0" minWidth="10.0" prefWidth="300.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Button fx:id="btnVref" alignment="CENTER" minWidth="80.0" mnemonicParsing="false" text="Vref" />
      <Button minWidth="80.0" mnemonicParsing="false" onAction="#pressButtonVout" text="Vout" GridPane.rowIndex="1" />
      <Button minWidth="80.0" mnemonicParsing="false" onAction="#pressButtonR1" text="R1" GridPane.rowIndex="2" />
      <Button minWidth="80.0" mnemonicParsing="false" onAction="#pressButtonR2" text="R2" GridPane.rowIndex="3" />
      <TextField fx:id="txtFieldVref" text="0" GridPane.columnIndex="1" />
      <TextField fx:id="txtFieldVout" text="0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="txtFieldR1" text="0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="txtFieldR2" text="0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   </children>
   <effect>
      <Blend />
   </effect>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</GridPane>
